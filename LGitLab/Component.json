{
    "name": "LGitLab",
    "version": "1.0",
    "classFile": "LGitLab.php",
    "className": "LGitLab",
    "files": [
        {"path": ".htaccess"},
        {"path": "LGitLab.php"},
        {"path": "index.php"},
        {"path": "Commands.json"},
        {"path": "info/"}
    ],
    "links": [
        {
            "name": "getUser",
            "target": "DBUser",
            "links": [{
                "method": "GET",
                "path": "/user/user/:userid"
            }]
        },
        {
            "name": "getGroup",
            "target": "DBGroup",
            "links": [{
                "method": "GET",
                "path": "/group/user/:userid/exercisesheet/:sheetid"
            }]
        },
        {
            "name": "postSubmission",
            "target": "LProcessor",
            "links": [{
                "method": "POST",
                "path": "/submission"
            }]
        },
        {
            "name": "getCourseExerciseSheets",
            "target": "LExerciseSheet",
            "links": [{
                "method": "GET",
                "path": "/exercisesheet/course/:courseid/exercise"
            }]
        }
    ],
    "connector":[
        {"name": "request","target": "CLocalObjectRequest"},
        {"name": "postPlatform","target": "CInstall"}
    ]
}